(* parallel streams (full-duplex communication) *)

protocol ParOne(role A, role B, role C) {
  par {
    rec X { M1 from A to B; continue X ; }
  } and {
    rec X { M2 from B to A; continue X ; }
}}


protocol ParTwo(role A, role B, role C) {
  par {
    rec X { M1 from A to B; continue X ; }
  } and {
    rec X { M2 from B to A; continue X ; }
  } and {
    rec X { M3 from A to C; continue X ; }
}}
